{
  "extends": "../../config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "target": "es5",
    "module": "esnext",
    "sourceMap": true,
    "lib": [
      "es6",
      "dom",
      "es2017",
      "esnext"
    ],
    "noImplicitThis": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "files": [
    "./typings/global.d.ts"
  ],
  "include": [
    "./page/**/**.tsx", "page/antd/store/config.ts",
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
